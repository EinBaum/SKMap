						<AbsDimension x="4" y="24" />
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$parentMiddle" />
					</Anchors>
					<TexCoords bottom="0.75" left="0.90625" right="0.96875" top="0" />
