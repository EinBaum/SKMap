				         name="$parentMiddle">
					<Size>
						<AbsDimension x="53" y="24" />
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$parentLeft" />
					</Anchors>
					<TexCoords bottom="0.75" left="0.078125" right="0.90625" top="0" />
				</Texture>

				<Texture file="Interface\FriendsFrame\WhoFrame-ColumnTabs"
				         name="$parentRight">
