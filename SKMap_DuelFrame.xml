						<AbsDimension x="35" y="14" />
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$parentWin">
							<Offset>
								<AbsDimension x="6" y="0" />
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>

				<FontString inherits="GameFontHighlightSmall" justifyH="LEFT"
